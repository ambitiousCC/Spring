

public class mathOpe {
	public static void main(String[] args) {
		int a = 13;         int b = 4;
		System.out.println(a + b); 
		System.out.println(a - b);
		System.out.println(a * b);
		System.out.println(a / b);
		System.out.println((float)a / b);
		System.out.println(a % b);
		int c = ++b;       int d = a--;
		System.out.println(c);
		System.out.println(d);
	}
}
